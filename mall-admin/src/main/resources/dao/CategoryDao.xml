<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzr.mall.dao.CategoryDao">

  <select id="selectAll" resultMap="com.zzr.mall.mapper.CategoryMapper.BaseResultMap">
    select * from category
  </select>
  <select id="selectById" parameterType="java.lang.Integer" resultMap="com.zzr.mall.mapper.CategoryMapper.BaseResultMap">
    select * from category where id=#{id}
  </select>
</mapper>